{
  "name": "flofgr/laravel-paybox-gateway",
  "description": "Laravel 6+ Paybox gateway",
  "require": {
    "laravel/framework": ">=5.8",
    "guzzlehttp/guzzle": "^6.2",
    "ext-openssl": "*",
    "ext-iconv": "*"
  },
  "autoload": {
    "psr-4": {
      "Devpark\\PayboxGateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "license": "MIT",  
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "mockery/mockery": "^0.9.5"
  }
}
