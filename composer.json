{
  "name": "maximeb/laravel-8-paybox-gateway",
  "description": "Laravel 8+ Paybox gateway",
  "require": {
    "laravel/framework": "~8.0",
    "guzzlehttp/guzzle": "~7.0",
    "ext-openssl": "*",
    "ext-iconv": "*"
  },
  "autoload": {
    "psr-4": {
      "Devpark\\PayboxGateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "mockery/mockery": "^0.9.5"
  }
}
